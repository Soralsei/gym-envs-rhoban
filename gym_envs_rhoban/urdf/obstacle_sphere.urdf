    <contact>
      <rolling_friction value="0.001"/> 
      <spinning_friction value="0.001"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value=".1"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
				<mesh filename="meshes/sphere.obj" scale="0.2 0.2 0.5"/>      
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </collision>
  </link>
</robot>